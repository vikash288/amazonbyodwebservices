<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>KonyRedshiftConnection</servlet-name>
  	<display-name>KonyRedshiftConnection</display-name>
  	<description></description>
  	<servlet-class>com.awsbyodwebservices.redshift.KonyRedshiftConnection</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RedShiftWebservices</servlet-name>
  	<display-name>RedShiftWebservices</display-name>
  	<description></description>
  	<servlet-class>com.byodwebservices.webservices.RedShiftWebservices</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>KonyRedshiftConnection</servlet-name>
  	<url-pattern>/KonyRedshiftConnection</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RedShiftWebservices</servlet-name>
  	<url-pattern>/RedShiftWebservices</url-pattern>
  </servlet-mapping>
</web-app>
